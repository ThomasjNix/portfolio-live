<link rel="stylesheet" type="text/css" href="<%= styleLink %>">
<% include ../partials/header %>
 <div class="row">
 	<div class="col s12 m8 offset-m2">
	   <div class="card">
	    <div class="card-tabs">
	      <ul class="tabs tabs-fixed-width">
	        <li class="tab"><a class="active" href="#skills">Skills</a></li>
	        <li class="tab"><a href="#projects">Projects</a></li>
	      </ul>
	    </div>
	    <div class="card-content grey lighten-4">
	      <div id="skills">
			<ul class="collection">
			
				<% skills.forEach(function(skill){ %>
					<li class="collection-item avatar">
						<img src="<%= skill.skillImageUrl %>" alt="" class="circle">
						<span class="title"><%= skill.skillName %></span>
						<p><%= skill.skillDescription %></p>
					</li>
				<% }) %>
				
			</ul>
	      	
	      </div>

	      <div id="projects">
	      	<ul class="collection">
				<% projects.forEach(function(project){ %>
					<li class="collection-item avatar">
						<img src="<%= project.projectImageUrl %>" alt="" class="circle">
						<span class="title"><%= project.projectName %></span>
						<p><%= project.projectDescription %></p>
					</li>
				<% }) %>
			</ul>
	      </div>
	    </div>
	  </div>
	</div>
</div>
<div class="row" id="add-buttons">
	<div class="col s6">
		<a href="/admin/skills/new" class="waves-effect waves-light btn green darken-3"><i class="material-icons left">add</i>Add Skill</a>
	</div>
	<div class="col s6">
		<a href="/admin/projects/new" class="waves-effect waves-light btn green darken-3"><i class="material-icons left">add</i>Add Project</a>
	</div>
</div>


<% include ../partials/footer %>