<% include ../../partials/header %>

 <div class="row">
    <form class="col s10 offset-s1" id="admin-form" action="/admin/skills?_method=PUT" method="POST">
      <div class="row">
        <div class="input-field col s12">
          <input id="skill_name" type="text" class="validate" name="skill[name]" placeholder="skill[skillName]">
          <label for="skill_name">Skill Name</label>
        </div>
        <div class="input-field col s12">
          <input id="skill_imgUrl" type="text" class="validate" name="skill[imageUrl]" placeholder="skill[skillImage]">
          <label for="skill_imgUrl">Skill Image URL</label>
        </div>
        <div class="input-field col s12">
          <textarea id="skill_description" class="materialize-textarea" name="skill[description]" value="skill[skillDescription]"></textarea>
          <label for="skill_description">Description</label>
        </div>

       	<button class="btn waves-effect waves-light" type="submit" name="action">Submit
	 		<i class="material-icons right">send</i>
		</button>
      </div>
     
    </form>
  </div>




<% include ../../partials/footer %>